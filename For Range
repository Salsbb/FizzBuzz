#initialize output to 1
output = 1

for i in range(1,100):
  if output == (i%3 == 0):    # if output == div by 3 no r, prints fizz
    print('fizz')
  elif output == (i%5 == 0):  # if output == dive by 5 no r, prints buzz
    print('buzz')
  else:                      # if not, print i
    print(i)
